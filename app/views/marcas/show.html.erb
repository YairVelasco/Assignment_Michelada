<table id="tablas", style="width:100%">
  <tr>
    <th>Product name</th>
    <th>Description</th>
    <th>Price</th>
    <th>Brand</th>
  </tr>
  
  <% @productos.each do |t| %>
    <tr>
      <td><%=t.name%></td>
      <td><%=t.description%></td>
      <td><%=t.price%></td>
      <td><%=@marca%></td>
    </tr>
  <% end %>
</table>